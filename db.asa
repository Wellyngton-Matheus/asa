;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	asa.labredes.br. root.asa.labredes.teste. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	asa.labredes.br.
@	IN	NS	asa1.labredes.br.

asa	IN	A	192.168.0.10 ; apontamento de dominio para ipv4


; servidorweb	IN	CNAME www.asa.labredes. ; exemplo de um apontamento do tipo CNAME
